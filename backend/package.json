{
  "name": "backend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec ./node_modules/.bin/babel-node -r dotenv/config src/server.js",
    "seed": "node prisma/seed.js",
    "build": "babel src -d dist",
    "start": "node -r dotenv/config dist/server.js",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint --fix src --ext .js",
    "format": "prettier --check \"{,!(node_modules)/**/}*.js\"",
    "format:fix": "prettier --write \"{,!(node_modules)/**/}*.js\""
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cloudinary": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "emailjs-com": "^3.2.0",
    "express": "^4.21.2",
    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "react-select": "^5.9.0",
    "react-toastify": "^11.0.3",
    "stripe": "^17.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "xlsx": "^0.18.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.23.5",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.5",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "prisma": "^5.9.0"
  }
}
